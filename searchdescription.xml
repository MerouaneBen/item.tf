<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
  <ShortName>TF2 Find</ShortName>
  <Description>Find TF2 items</Description>
  <Url type="text/html"
       template="http://www.tf2find.com/search?q={searchTerms}"/>
  <Url type="application/x-suggestions+json"
       template="http://www.tf2find.com/suggest?q={searchTerms}"/>
  <Url type="application/opensearchdescription+xml"
       rel="self"
       template="http://www.tf2find.com/searchdescription.xml" />
</OpenSearchDescription>
