<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
  <ShortName>item.tf</ShortName>
  <Description>Find TF2 items</Description>
  <Url type="text/html"
       template="https://item.tf/search?q={searchTerms}"/>
  <Url type="application/x-suggestions+json"
       template="https://item.tf/suggest?q={searchTerms}"/>
  <Url type="application/opensearchdescription+xml"
       rel="self"
       template="https://item.tf/searchdescription.xml" />
</OpenSearchDescription>
